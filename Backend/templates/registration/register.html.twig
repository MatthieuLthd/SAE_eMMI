{# templates/registration/register.html.twig #}

{% extends 'base.html.twig' %}

{% block body %}
    <h1>Inscription</h1>

    {% for message in app.flashes('error') %}
        <div class="alert alert-danger">
            {{ message|raw }}
        </div>
    {% endfor %}

    {{ form_start(form) }}
        {{ form_widget(form) }}
    {{ form_end(form) }}
{% endblock %}
